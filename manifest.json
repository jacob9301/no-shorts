{
    "manifest_version" : 3,
    "name" : "No Shorts",
    "version" : "0.1.0",
    "content_scripts" : [
        {
            "matches" : ["*://www.youtube.com/*"],
            "js" : ["script.js"],
            "run_at" : "document_end",
            "css" : ["styles.css"]
        }
    ],   
    "permissions": [
        "webNavigation",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
      }
}